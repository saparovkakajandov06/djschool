# Git global setup

git config --global user.name "Зохраб Абдуллаев"
git config --global user.email "zabdullayev@gmail.com"

# Create a new repository

git clone https://gitlab.com/zabdullayev/djscul.git
cd djscul
git switch -c main
touch README.md
git add README.md
git commit -m "add README"
git push -u origin main

# Push an existing folder
cd existing_folder
git init --initial-branch=main
git remote add origin https://gitlab.com/zabdullayev/djscul.git
git add .
git commit -m "Initial commit"
git push -u origin main

# Push an existing Git repository
cd existing_repo
git remote rename origin old-origin
git remote add origin https://gitlab.com/zabdullayev/djscul.git
git push -u origin --all
git push -u origin --tags



